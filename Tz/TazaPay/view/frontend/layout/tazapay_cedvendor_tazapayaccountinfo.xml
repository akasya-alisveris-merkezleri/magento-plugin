		<referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Tazapay Account Information</argument>
            </action>
        </referenceBlock>
		<referenceBlock name="vendor.dashboard.wrapper">
			<block class="Tz\TazaPay\Block\CedVendor\TazaPayAccountInfo" cacheable="false" name="tazapay_cedvendor_tazapayaccountinfo" template="Tz_TazaPay::CedVendor/tazapayAccountInfo.phtml" />
		</referenceBlock>
	</body>
</page>

